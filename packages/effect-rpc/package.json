{
  "name": "effect-rpc",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "version": "0.6.0",
  "author": {
    "name": "Benjamin Kraatz",
    "email": "benjamin-kraatz@example.com"
  },
  "license": "Apache-2.0",
  "keywords": [
    "effect",
    "rpc",
    "typescript",
    "monorepo",
    "effect-rpc",
    "rpc",
    "remote procedure call"
  ],
  "homepage": "https://github.com/benjamin-kraatz/effect-rpc",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjamin-kraatz/effect-rpc.git",
    "directory": "packages/effect-rpc"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "prebuild": "cp ../../README.md .",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
